pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--------------------------------------------------------------------------------
--------------------------------- global variables  ----------------------------
--------------------------------------------------------------------------------
player = {}
player.sprite = 0
player.x = 8
player.y = 8
player.speed = 3
player.angle = 0.5
player.turnspeed = 0.01

map_ = {}
map_.border = {}
map_.border.left = 0
map_.border.up = 0
map_.border.right = 120
map_.border.down = 120
map_.sx = 0
map_.sy = 0
map_.movemap = false


--------------------------------------------------------------------------------
-------------------------------- helper functions ------------------------------
--------------------------------------------------------------------------------
function debug()
  print("px: " .. player.x, 0, 0, 7)
  print("py: " .. player.y, 30, 0, 7)
  print("sx: " .. map_.sx, 0, 6, 7)
  print("sy: " .. map_.sy, 30, 6, 7)
end

function bump(x, y)
  return fget( mget( flr( (x - map_.sx) / 8 ), flr( (y - map_.sy) / 8 ) ), 0 )
end

-- https://www.lexaloffle.com/bbs/?tid=2592
function spra(angle, n, x, y, w, h, flip_x, flip_y)
  if w == nil or h == nil then
    w, h = 1, 1
  end
  flip_x = flip_x or false
  flip_y = flip_y or false

  local nx, ny, cosa, sina, h2, w2 = n % 16 * 8, flr(n / 16) * 8, cos(angle), sin(angle), 8 * h - 1, 8 * w - 1
  local h3, w3 = h * 4, w * 4
  for i = 0, w2 do
    local cx, sx, sprx = cosa * (i - w3), sina * (i - w3), flip_x and nx + w2 - i or nx + i
    for j = 0, h2 do
      local col = sget(sprx, flip_y and ny + h2 - j or ny + j)
      if col != 0 then
        pset(x + w3 + cx - sina * (j - h3), y + h3 + sx + cosa * (j - h3), col)
      end
    end
  end
end

--------------------------------------------------------------------------------
---------------------------------- constructor ---------------------------------
--------------------------------------------------------------------------------
function _init()

end --end _init()


--------------------------------------------------------------------------------
---------------------------------- update --------------------------------------
--------------------------------------------------------------------------------
function _update()
  --[[
    left arrow
  ]]
  if (btn(0)) then
    if map_.movemap then
      map_.sx -= 1
    else

      player.angle += player.turnspeed
    end
  end --end left button

  --[[
    right arrow
  ]]
  if (btn(1)) then
    if map_.movemap then
      map_.sx += 1

    else
      player.angle -= player.turnspeed
    end
  end --end right button

  --[[
    up arrow
  ]]
  if (btn(2)) then
    if map_.movemap then
      map_.sy -= 1
    else
      for i = 1, player.speed do
        if player.y < map_.border.up + 1
        or bump(player.x, player.y - 1)
        or bump(player.x + 7, player.y - 1)
        then break end

        player.y -= 1
      end
    end
  end --end up button

  --[[
    down arrow
  ]]
  if (btn(3)) then
    if map_.movemap then
      map_.sy += 1
    else
      for i = 1, player.speed do
        if player.y > map_.border.down - 1
        or bump(player.x, player.y + 8)
        or bump(player.x + 7, player.y + 8)
        then break end

        player.y += 1
      end
    end
  end --end down button

  --[[
    z button
  ]]
  if (btn(4)) then
    map_.movemap = true
  else
    map_.movemap = false
  end --end z button

  --[[
    x button
  ]]
  if (btn(5)) then
    map_.sx = 0
    map_.sy = 0
    player.x = 8
    player.y = 8
    player.angle = 0.5
  end --end x button

end --end _update()


--------------------------------------------------------------------------------
--------------------------------- draw buffer ----------------------------------
--------------------------------------------------------------------------------
function _draw()
  cls()

  map(0, 0, map_.sx, map_.sy, 128, 128)

  spra(player.angle, player.sprite, player.x, player.y)

  --debug()
end --end _draw()


